        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "aei784ojzbrpca"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  time_bucket('10 seconds', e.timestamp) AS \"time\",\n  AVG(e.power_watts) AS \"Power Consumption\",\n  AVG(p.nozzle_temp_actual) AS \"Hotend Temperature\"\nFROM energy_data e\nLEFT JOIN LATERAL (\n  SELECT nozzle_temp_actual\n  FROM printer_status\n  WHERE device_id = e.device_id\n    AND timestamp <= e.timestamp\n  ORDER BY timestamp DESC\n  LIMIT 1\n) p ON true\nWHERE e.device_id != 'environment'\n  AND $__timeFilter(e.timestamp)\nGROUP BY 1\nORDER BY 1 ASC;",
          "refId": "A"
        }
      ],
      "title": "Power & Temperature Correlation",
      "description": "Dual-axis visualization showing power consumption correlated with hotend temperature. Identifies heating efficiency and thermal patterns.",
      "type": "timeseries"
    },
